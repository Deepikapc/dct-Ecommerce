<h2> Edit Category </h2>
<%= form_for @category do |f| %>
<% if @category.errors.any? %>
<ul>
  <% @category.errors.full_messages.each do |msg| %>
  <h2> Errors: </h2>
   <li> <%= msg %> </li>
  <% end %>
  </ul>
<% end %>
<%= f.label :name %>
<%=f.text_field :name %> </br>

<%= f.submit %> | <%= link_to "Back",category_path(@category.id) %> | <%= link_to "List" , categories_path %>

<% end %>